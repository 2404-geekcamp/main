{
  "name": "react-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "set PORT=3002 && npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "test": "jest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.42.5",
    "axios": "^1.6.8",
    "concurrently": "^8.2.2",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "dotenv": "^16.4.5",
    "express": "~4.19.2",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "nodemon": "^3.1.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
